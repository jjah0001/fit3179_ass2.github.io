{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width":490,
    "title": "English Proficiency Index of Each Country ordered in terms of the Ease of Business Index",
    "height": {"step": 6},
    "align": "all",
    "data": { "url": "https://raw.githubusercontent.com/jjah0001/fit3179_ass2.github.io/master/data/JoinedData3.csv"},
    "mark": "bar",
    "encoding": {
      "y": {
        "field": "Country",
        "type": "nominal",
        "sort": {"field":"Rank", "op": "sum"}
      },
      "x": {
        "aggregate": "sum",
        "field": "Score",
        "title": "English Proficiency Index",
        "scale": {"domain": [350, 670]}
      }
    }
  }